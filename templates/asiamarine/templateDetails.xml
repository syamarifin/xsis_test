<config>
<fields name="params">
<fieldset name="Basic" label="Configuration">
<field name="logo" type="media" label="Logo" default="templates/asiamarine/images/logo.png" />
<field name="style" type="list" default="style1" label="Preset Styles">
<option value="style1">Blue</option>
<option value="style2">Green</option>
<option value="style3">Black</option>
<option value="style4">Red</option>
<option value="style5">Yellow</option>
</field>
<field name="font" type="list" default="ff1" label="Font Family">
<option value="ff1">Arial</option>
<option value="ff2">Times New Roman</option>
<option value="ff3">Courier New</option>
</field>
<field name="copy" type="text" default="" label="Copyright" />
<field name="show" type="radio" default="yes" class="btn-group btn-group-yesno" label="Design By">
<option value="yes">Show</option>
<option value="no">Hide</option>
</field>
</fieldset>
</fields>
</config>
</extension>